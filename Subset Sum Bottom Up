
int SubsetSum(vector<int> &A, int B) {
    
    vector< vector<int> > dp( A.size()+1, vector<int>(B+1,0) );
    
    for(int i=1;i<=A.size();i++)
    {
        for(int j=1;j<=B;j++)
        {
            if(A[i-1]>j)
            {
                dp[i][j]=dp[i-1][j];
            }
            else
            {
                dp[i][j]=max(dp[i-1][j], A[i-1]+dp[i-1][j-A[i-1]]);
            }
        }
        
    }
    if(dp[A.size()][B]==B)return 1;
    return 0;
}
